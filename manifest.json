{
    "manifest_version": 3,
    "name": "QuickCaptcha",
    "version": "1.0",
    "description": "Automatically fills the captcha.",
    "author": "Astra",
    "homepage_url": "https://github.com/kartikshriwas",
    "developer": {
        "name": "Kartik Shriwas",
        "email": "kartikshriwas107@gmail.com"
    },
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["index.js"],
        "run_at": "document_end"
      }
    ]
  }